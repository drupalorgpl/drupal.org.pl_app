{
    "name": "droptica/droopler-project",
    "description": "Project template for Droopler with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "features": {
            "type": "package",
            "package": {
                "name": "drupal/features",
                "version": "3.13.0",
                "type": "drupal-module",
                "dist": {
                    "type": "zip",
                    "url": "https://git.drupalcode.org/issue/features-3447460/-/archive/40732dfd317473d9913c9e97d0cf7e4d68a78934/features-3447460-40732dfd317473d9913c9e97d0cf7e4d68a78934.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/features-3447460.git",
                    "reference": "40732dfd317473d9913c9e97d0cf7e4d68a78934"
                },
                "require": {
                    "drupal/core": "^9.4 || ^10 || ^11"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/features"
            ]
        },
        "asset": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "droptica/d_application_api": "^1.1",
        "droptica/droopler": "^3.5.3",
        "drupal/core": "^11.0",
        "drupal/core-composer-scaffold": "^11",
        "drupal/core-recommended": "^11",
        "drupal/devel": "^5.0",
        "drupal/entity": "^1.6",
        "drupal/eu_cookie_compliance": "^1.25",
        "drupal/lazy": "^4.0",
        "drupal/linkit": "^7",
        "drupal/mysql57": "^1.0",
        "drupal/rdf": "^3",
        "drupal/upgrade_status": "^4.3",
        "drupal/we_megamenu": "^1.16",
        "drush/drush": "^13",
        "oomphinc/composer-installers-extender": "^2.0",
        "webflo/drupal-finder": "^1.3",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^11"
    },
    "conflict": {
        "drupal/drupal": "*",
        "droptica/droopler": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:git",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "droptica/d_application_api": {
                "Add support for Drupal 10": "patches/d_application_api/d10-compatibility.patch"
            }
        }
    }
}
